    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "npm": "^7.11.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.3",
    "ts-node": "^9.1.1",
