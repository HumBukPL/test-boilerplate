    "@types/express": "^4.17.11",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.24.0",
    "autoprefixer": "^10.2.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.2",
